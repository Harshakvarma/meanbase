.mdl-grid
  .mdl-cell.mdl-cell--4-col.mdl-cell--4-col-tablet
    .form-group
      label Google Analytics App ID
      input(type='text' ng-model="appID").form-control
      button.btn.btn-success(ng-click="changeAppID(appID)").btn-block Set App ID
  .mdl-cell.mdl-cell--4-col.mdl-cell--4-col-tablet
    .form-group
      label Google Analytics Client ID
      input(type='text' ng-model="clientID").form-control
      button.btn.btn-success(ng-click="changeClientID(clientID)").btn-block Set Client ID
  .mdl-cell.mdl-cell--4-col.mdl-cell--8-col-tablet
    .form-group
      label Site Verification ID [for email]
      input(type='text' ng-model="verificationID").form-control
      button.btn.btn-success(ng-click="changeVerificationID(verificationID)").btn-block Set Verification ID
  .mdl-cell.mdl-cell--6-col.mdl-cell--4-col-tablet
    .form-group
      label Google Recaptcha Server Key
      input(type='text' ng-model="recaptchaKey").form-control
      button.btn.btn-success(ng-click="changeRecaptcha()").btn-block Set Server Recaptcha Key
  .mdl-cell.mdl-cell--6-col.mdl-cell--4-col-tablet
    .form-group
      label Google Recaptcha Client Key
      input(type='text' ng-model="recaptchaClientKey").form-control
      button.btn.btn-success(ng-click="changeClientRecaptcha()").btn-block Set Client Recaptcha Key
