    .dropdown
      position: relative
      box-sizing: border-box
      ul
        margin 0

    .dropdown-list
      display block
      visibility: hidden
      min-width: 170px
      position: absolute
      left: 0
      top: 100%
      padding 0.5em 0
      background-color white
      border-radius: 3px
      box-shadow: 2px 2px 2px 1px rgba(0, 0, 0, 0.2)

    .dropdown-list li
      list-style: none
      color #2C3E50
      cursor: pointer

      i
      a
        display block
        font-size: 16px
        font-weight: normal
        color #2C3E50
        text-decoration: none
        cursor pointer
        padding 0.3em 1em

      &:hover a
        color white

    .dropdown-list li.divider
      background-color: #dedede
      margin 0 0.5em
      height 2px
      padding 0

    .dropdown-list li:hover
      background-color: #2980B9

    .dropdown-active
      z-index: 100
      visibility: visible

    a
      cursor pointer


