.mdl-grid
  .mdl-cell.mdl-cell--12-col
    div.drop-zone(nv-file-drop="uploader" nv-file-over multiple uploader="uploader").text-center.form-inline
      p Drop files here to upload or
      .form-group.text-center
        input.form-control(type='file' nv-file-select multiple uploader="uploader")
      .progress
          .progress-bar(role="progressbar" ng-style="{ 'width': uploader.progress + '%' }")
.mdl-grid
  .mdl-cell.mdl-cell--12-col
    image-selector(image-selector-config="imageSelectorConfig")
